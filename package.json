        "@react-email/html": "0.0.11",
        "@react-email/render": "1.1.3",
        "@swc/core": "1.13.3",
        "@types/react-dnd": "3.0.2",
        "bcrypt": "6.0.0",
        "bootstrap": "5.3.7",
        "date-fns": "4.1.0",
        "dayjs": "1.11.13",
        "debounce": "2.2.0",
        "exceljs": "4.4.0",
        "iron-session": "8.0.4",
        "jsonwebtoken": "9.0.2",
        "next": "15.4.3",
        "next-international": "1.3.1",
        "nodemailer": "7.0.5",
        "prisma": "6.12.0",
        "react": "19.1.0",
        "react-bootstrap": "2.10.10",
        "react-date-picker": "11.0.0",
