        "date-fns": "4.1.0",
        "dayjs": "1.11.19",
        "debounce": "2.2.0",
        "exceljs": "4.4.0",
        "iron-session": "8.0.4",
        "jsonwebtoken": "9.0.3",
        "next": "15.5.9",
        "next-international": "1.3.1",
        "nodemailer": "7.0.12",
        "prisma": "6.19.1",
        "react": "19.2.3",
