{
    "name": "uniform-administration",
    "version": "2.4.1",
    "private": true,
    "engines": {
        "node": "22.14.0"
    },
    "scripts": {
        "dev": "next dev -p 3021",
        "build": "next build",
        "build:test": "cross-env NEXT_PUBLIC_STAGE=TEST next build",
        "start": "next start -p 3021",
        "test:e2e": "playwright test",
        "test:jest": "npm run test:dal && npm run test:components",
        "test:components": "jest --config jest.frontend.config.ts",
        "test:dal": "npm run test:dal:unit && npm run test:dal:integration",
        "test:dal:unit": "jest --config jest.dal-unit.config.ts",
        "test:dal:integration": "jest --config jest.dal-integration.config.ts",
        "test:watch:components": "jest --config jest.frontend.config.ts --watch",
        "test:watch:dal:unit": "jest --config jest.dal-unit.config.ts --watch",
        "test:watch:dal:integration": "jest --config jest.dal-integration.config.ts --watch",
        "test:coverage:all": "npm run test:all -- --coverage",
        "test:coverage:components": "jest --config jest.frontend.config.ts --coverage",
        "test:coverage:dal": "jest --config jest.dal-unit.config.ts --coverage",
        "lint": "eslint src tests"
    },
    "prisma": {
        "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "6.7.2",
        "@fortawesome/free-regular-svg-icons": "6.7.2",
